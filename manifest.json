{
  "manifest_version": 3,
  "name": "Hello Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "permissions": ["sidePanel", "tabs", "storage"],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_title": "Click to open Scopify panel"
  },
  "side_panel": {
    "default_path": "sidepanel/side_panel.html"
  }
}
